{
  "version": "1.0.0-*",

  "dependencies": {
    "DockerDash": {
      "version": "1.0.0-*",
      "target": "project"
    },
    "dotnet-test-xunit": "1.0.0-rc2-*",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "xunit": "2.1.0",
    "NETStandard.Library": "1.6.0"
  },
  "buildOptions": {
    "copyToOutput": {
      "include": [ "appsettings.json" ]
